{
	"id": "chromatone-jam",
	"name": "Jam Panel",
	"short_name": "Jam Panel",
	"description": "Fullscreen Jam control panel",
	"start_url": "/",
	"background_color": "#999",
	"theme_color": "#999",
	"display": "standalone",
	"orientation": "any",
	"display_override": ["window-controls-overlay", "minimal-ui", "standalone"],
	"handle_links": "preferred",
	"prefer_related_applications": false,
	"categories": ["music", "audio", "tools"],
	"icons": [
		{
			"src": "logo.png",
			"type": "image/png",
			"sizes": "any"
		},
		{
			"src": "logo.svg",
			"type": "image/xml+svg",
			"sizes": "any"
		}
	],
	"launch_handler": {
		"client_mode": ["focus-existing", "auto"]
	}
}
